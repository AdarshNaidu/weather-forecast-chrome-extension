{
    "manifest_version" : 2,
    "name" : "weather forecast",
    "description" : "shows forecast",
    "version" : "1",
    "icons" :{
        "128": "icon.png"
    },
    "browser_action" :{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "weather"
    },
    "permissions": ["activeTab", "geolocation"]
}